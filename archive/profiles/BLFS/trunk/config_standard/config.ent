<!-- The directory that contains the packages -->
<!ENTITY packages_dir "/usr/src/packages">

<!-- The directory in which the packages will be compiled -->
<!ENTITY build_dir "/usr/src/build">

<!-- Your kernel version -->
<!ENTITY kernel-version "2.6.11.12">

<!-- Your kernel directory -->
<!ENTITY kernel-directory "/usr/src/linux-2.6.11.12">

<!-- Network device -->
<!ENTITY network-interface "eth0">

<!-- Configuration for the stunnel build -->
<!ENTITY stunnel-country "CA">
<!ENTITY stunnel-state   "Alberta">
<!ENTITY stunnel-city    "Calgary">
<!ENTITY stunnel-org     "Linux From Scratch">
<!ENTITY stunnel-orgunit "ALFS">
<!ENTITY stunnel-host    "localhost">

<!-- XFree86 configuration -->
<!ENTITY mouseProtocol "IMPS/2">

<!ENTITY XkbModel  "pc101">
<!ENTITY XkbLayout "fr">
 
<!ENTITY HorizSync   "31.5-57.0">
<!ENTITY VertRefresh "50-92">
<!ENTITY Modeline    '"1152x864" 92 1152 1208 1368 1474 864 865 875 895'>

<!ENTITY Driver     "nv">
<!ENTITY VendorName "NVidia">
<!ENTITY BoardName  "Riva TNT2 M64">
<!ENTITY BusID      "PCI:1:0:0">

<!-- Default non-root user of your system -->
<!-- (Used to place some configuration files, and to send mail to.) -->
<!ENTITY username  "non-root-user">

<!-- The fully qualified host name used by postfix -->
<!ENTITY hostname "your.hostname">
<!ENTITY domain   "your.domain">

<!-- Postfix additional args. See the BLFS book for details -->
<!ENTITY postfix-ccargs  "">
<!ENTITY postfix-auxlibs "">

<!-- Initial root password for mysql -->
<!ENTITY mysql-password "alpha">

<!-- Cracklib default wordlist -->
<!ENTITY cracklib-wordlist "allwords">

<!-- Entities used to build tripwire: -->
<!-- The name of your policy file for tripwire -->
<!ENTITY tripwire-policy-file "twpol.txt">
<!-- Usable values to pass to make -->
<!ENTITY tripwire-mail-domain "mail.domain.com">
<!ENTITY tripwire-smtp-port "25">
<!ENTITY tripwire-sitepassphrase "blahville">
<!ENTITY tripwire-localpassphrase "blahville2">

<!-- Perl version --> 
<!-- Needed for perl modules -->
<!ENTITY perl-version "5.8.6">

<!-- Net-DNS nameserver tests -->
<!-- Whether you want to perform these tests -->
<!ENTITY nameserver-tests "yes">

<!-- Kerberos implementation -->
<!-- MIT-krb 5 (mitkrb) or Heimdal (heimdal) -->
<!-- MIT-krb 5 is all but depreciated in BLFS-6.1 -->
<!ENTITY krb-impl "heimdal">

<!-- Java implementation -->
<!-- binary (jdk-binary) or source (jdk-source) -->
<!ENTITY jdk "jdk-source">

<!-- X implementation -->
<!-- XFree86 (xfree86) or X.org (xorg) -->
<!ENTITY ximpl "xorg">

<!-- Qt install location -->
<!-- Qt can either be installed in either the /usr or /opt hierarchy 
  configure by setting the following entity to "qt" for /usr or "qt-opt" for
  /opt -->
<!ENTITY qt-install "qt">

<!-- Ghostscript implementation -->
<!-- esp-gs (esp-ghostscript) or afpl-gs (ghostscript)-->
<!ENTITY ghostscript-impl "esp-ghostscript">

<!-- GIF library implementation-->
<!-- libungif or giflib -->
<!ENTITY gif-impl "giflib">

<!-- MTA implementation -->
<!-- sendmail, exim, courier, qpopper or postfix -->
<!ENTITY mta-impl "sendmail">

<!-- MySQL root password -->
<!ENTITY mysql-root-pass "blahblah">

<!-- KDE configuration -->
<!-- Set here your language-->
<!-- ar, bg, bn, br, bs, ca, cs, cy, da, de, el, en_GB, eo, es, et, eu, fi, fr, fy, ga, he, hi, hsb, hu, is, it,
ja, lt, mk, nb, nds, nl, nn, pa, pl, pt, pt_BR, ro, ru, se, sk, sl, sr, sr@Latn, sv, ta, tg, tr, uk, zh_CN -->
<!ENTITY kdelang "en_GB">

<!-- KDE Install Type -->
<!-- Set kde-install to usr or opt, also remember to change the kde-prefix
  entity below accordingly (if you set kde-install to opt set kde-prefix to /opt/kde-3.4.1)-->
<!ENTITY kde-install "usr">
<!ENTITY kde-prefix "/usr">

<!-- GNOME configuration -->
<!-- GNOME Install Type -->
<!-- Set to "usr" or "opt", also remember to change the gnome-prefix
  entity below accordingly --> 
<!ENTITY gnome-install "usr">
<!ENTITY gnome-prefix "/usr">

<!-- ALSA driver -->
<!ENTITY sound-card  "emu10k1">

<!-- MPlayer font size -->
<!ENTITY mplayer-font-size "14">

